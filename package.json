{
  "version": "0.7.0-19",
  "scripts": {
    "release": "run-s check build my_publish",
    "check": "run-p lint test",
    "pre-commit": "run-p lint-fix test",
    "lint": "xo",
    "lint-fix": "xo --fix",
    "test": "mocha",
    "build": "unbuild",
    "my_publish": "np --yolo"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "hamjest": "^4.1.0",
    "mocha": "^10.3.0",
    "np": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.2",
    "unbuild": "^2.0.0",
    "xo": "^0.24.0"
  },
  "devDependencies_doc": {
    "typescript": "required by unbuild, even if unused"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "mocha": {
    "spec": "src/**/*.test.mjs",
    "reporter": "dot"
  },
  "xo": {
    "envs": [
      "node",
      "mocha"
    ],
    "prettier": true,
    "rules": {
      "capitalized-comments": "off"
    }
  },
  "name": "kiss-and-type",
  "description": "A lightweight approach to typing in plain JavaScript",
  "keywords": [
    "fp",
    "functional",
    "types"
  ],
  "author": "Alex Bepple <alex@bepple.de> (http://alex.bepple.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbepple/kiss-and-type.git"
  }
}
